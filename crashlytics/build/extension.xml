<?xml version="1.0" encoding="UTF-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/4.0">
  <id>com.github.airext.fabric.Crashlytics</id>
  <versionNumber>1.0.0</versionNumber>
  <platforms>
    <platform name="iPhone-ARM">
      <applicationDeployment>
        <nativeLibrary>libCrashlytics.a</nativeLibrary>
        <initializer>ANXCrashlyticsInitializer</initializer>
        <finalizer>ANXCrashlyticsFinalizer</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="default">
      <applicationDeployment />
    </platform>
  </platforms>
</extension>